{
  "welcomeFile": "/ItkApp/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/backend/(.*)$",
      "target": "$1",
      "authenticationType": "xsuaa",
      "destination": "backend"
    },
    {
      "source": "^/ItkApp/(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "authenticationType": "xsuaa"
    }
  ]
}